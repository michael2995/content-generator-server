{
  "name": "content-generator-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "yarn nodemon --exec 'ts-node' ./src/local.ts",
    "clean": "rm -rf ./dist ./node_modules",
    "build": "rm -rf ./dist && yarn tsc",
    "gulp:sls:dev": "yarn gulp --require ts-node/register --f gulp.sls.dev.ts",
    "build:sls:dev": "yarn build && yarn gulp:sls:dev && cd dist && yarn install --production",
    "deploy:sls:dev": "yarn build:sls:dev && cd dist && sls deploy"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/gulp": "^4.0.8",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/uuid": "^8.3.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^2.0.7",
    "puppeteer": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@vendia/serverless-express": "^4.3.4",
    "add": "^2.0.6",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.876.0",
    "chrome-aws-lambda": "^8.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "puppeteer-core": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "serverless-http": "^2.7.0",
    "uuid": "^8.3.2"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/*.ts",
      "src/*.tsx",
      "src/**/*.ts",
      "src/**/*.tsx"
    ]
  }
}
